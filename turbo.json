{
  "baseBranch": "origin/main",
  "pipeline": {
    "lint": {
      "outputs": []
    },
    "dev": {
      "dependsOn": ["^dev", "db#db:generate", "db#db:push"],
      "cache": false
    },
    "build": {
      "dependsOn": ["^build", "db#db:generate"],
      "outputs": [".next/**", "dist/**", "build/**"],
      "outputMode": "new-only"
    },
    "docs#build": {
      "dependsOn": ["api:generate"],
      "outputs": ["build/**"],
      "outputMode": "new-only"
    },
    "build:env": {
      "cache": false
    },
    "api:generate": {
      "dependsOn": ["db#db:generate"],
      "cache": false
    },
    "db:generate": {
      "cache": false
    },
    "db:push": {
      "cache": false
    }
  }
}
